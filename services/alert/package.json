        "@tralert/express-app": "^1.0.0",
        "@tralert/logger": "^1.0.0",
        "@tralert/middlewares": "^1.0.0",
        "@tralert/utils": "^1.0.0",
        "auth0": "^2.26.0",
        "axios": "^0.19.2",
        "cron": "^1.8.2",
        "easy-table": "^1.1.1",
        "express": "^4.17.1",
        "express-mongo-sanitize": "^2.0.0",
        "http-status": "^1.4.2",
        "lodash.pick": "^4.4.0",
        "moment": "^2.26.0",
        "mongoose": "^5.9.18",
        "nodemailer": "^6.4.8"
