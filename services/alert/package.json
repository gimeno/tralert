{
    "name": "@tralert/alert",
    "version": "1.0.0",
    "description": "Service to handle alerts",
    "scripts": {
        "start": "node src/server.js",
        "dev": "nodemon -r dotenv/config --watch ../../packages --watch . src/server.js",
